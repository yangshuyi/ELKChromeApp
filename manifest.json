{
    "manifest_version": 2,
    "name": "ELKChromeApp",
    "description": "ELKChromeApp",
    "version": "0.1.0",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "16": "images/logo16.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    },
    "permissions": [
        "webview",
        "<all_urls>"
    ]
}
